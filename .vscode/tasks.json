    {
      "label": "Clean Build Runner",
      "type": "shell",
      "command": "dart run build_runner clean"
    },
    {
      "label": "Build Build Runner",
      "type": "shell",
      "command": "dart run build_runner build --delete-conflicting-outputs"
    },
    {
      "label": "Setup Locale",
      "type": "shell",
      "command": "dart run bin/generate.dart -S assets/translations -f keys -O lib/src/utils -o locale_keys.g.dart"
    },
    {
      "label": "Setup Package Name Staging",
      "type": "shell",
      "command": "dart run package_rename --flavor=staging",
      "dependsOn": [
        // "Setup Package Google Services Staging",
        "Clean Build Runner",
        "Build Build Runner",
        "Setup Locale"
      ],
      "dependsOrder": "sequence"
    },
    {
      "label": "Setup Package Name Production",
      "type": "shell",
      "command": "dart run package_rename --flavor=production",
      "dependsOn": [
        // "Setup Package Google Services Production",
        "Clean Build Runner",
        "Build Build Runner",
        "Setup Locale"
      ],
      "dependsOrder": "sequence"
    }
  ]
}
